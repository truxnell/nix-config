    {
      "matchDatasources": ['docker'],
      "automerge": "true",
      "automergeType": "branch",
      "matchUpdateTypes": [ 'minor', 'patch', 'digest'],
      "matchPackageNames": [
        'onedr0p/sonarr',
        'onedr0p/readarr',
        'onedr0p/radarr',
        'onedr0p/lidarr',
        'onedr0p/prowlarr'
      ]
    },
    {
      "matchDatasources": ['docker'],
      "automerge": "true",
      "automergeType": "branch",
      "matchUpdateTypes": [ 'patch', 'digest'],
      "matchPackageNames": [
        "gethomepage/homepage"
      ]

    }
  ]
}
